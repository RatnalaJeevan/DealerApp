<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">

    <RelativeLayout
        android:id="@+id/rl_page"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rl_top"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginTop="25dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="30dp">
        <ImageView
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:src="@drawable/wd_new_logo">

        </ImageView>
        <ImageView

            android:id="@+id/iv_filter"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:src="@drawable/filter_black"
            >

        </ImageView>

    </RelativeLayout>

    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rl_top"
        android:layout_above="@+id/card_btm_nav">

    </FrameLayout>
    <androidx.cardview.widget.CardView
        android:id="@+id/card_btm_nav"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:elevation="5dp"
        android:layout_alignParentBottom="true"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal">
            <LinearLayout
                android:id="@+id/layout_1"
                android:layout_width="wrap_content"
                android:layout_height="37dp"
                android:layout_weight="1"
                android:layout_marginTop="11dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal"
                android:gravity="center">

                <ImageView
                    android:id="@+id/iv_home"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:src="@drawable/selcted_home" />
                <TextView
                    android:id="@+id/tv_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Home"
                    android:textColor="@color/black"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="3dp"
                    android:textSize="12dp"
                    android:visibility="visible"
                    android:fontFamily="@font/poppins_regular"/>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout_2"
                android:layout_width="wrap_content"
                android:layout_height="37dp"
                android:layout_marginTop="11dp"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginRight="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/iv_pack"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:src="@drawable/unselected_pack"/>
                <TextView
                    android:id="@+id/tv_pack"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Package"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:layout_marginLeft="10dp"
                    android:visibility="gone"
                    android:fontFamily="@font/poppins_regular"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout_3"
                android:layout_width="wrap_content"
                android:layout_height="37dp"
                android:layout_marginTop="11dp"
                android:layout_weight="1"
                android:layout_marginRight="10dp"
                android:orientation="horizontal"
                android:gravity="center" >

                <ImageView
                    android:id="@+id/iv_customer"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:src="@drawable/unslected_cust"/>
                <TextView
                    android:id="@+id/tv_customer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Customer"
                    android:textSize="12dp"
                    android:textColor="@color/black"
                    android:layout_marginLeft="10dp"
                    android:visibility="gone"
                    android:fontFamily="@font/poppins_regular"/>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout_4"
                android:layout_width="wrap_content"
                android:layout_height="37dp"
                android:layout_marginTop="11dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:gravity="center">

                <ImageView
                    android:id="@+id/iv_profile"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:src="@drawable/profile_icon"/>
                <TextView
                    android:id="@+id/tv_profile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Profile"
                    android:textSize="12dp"
                    android:textColor="@color/black"
                    android:layout_marginLeft="10dp"
                    android:visibility="gone"
                    android:fontFamily="@font/poppins_regular"/>

            </LinearLayout>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    </RelativeLayout>


</RelativeLayout>